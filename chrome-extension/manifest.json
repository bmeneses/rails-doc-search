{
  "name": "Rails Doc Search",
  "version": "0.1",
  "manifest_version": 1,
  "description": "Search the Ruby on Rails API Documentation",
  "permissions": [
    "tabs",
    "http://api.rubyonrails.org/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "omnibox": {
    "keyword": "rd"
  },
  "background_page": "background.html"
}